<!DOCTYPE html>

<html lang="en">
  <head>
    <title>Famous Cats</title>
    <link rel="icon" type="image/png" href="images/cat.png" />
    <link rel="stylesheet" href="styles.css" />
  </head>

  <body>
    <header>
      <h1>Welcome to Famous Cats</h1>
    </header>
    <nav class="navbar">
      <a href="index.html">Home</a>
      <a href="jiji.html">Jiji</a>
      <a href="lion.html">Lion</a>
    </nav>

    <div class="container">
      <% for (let cat of cats) { %>
      <article class="cats">
        <div>Name: <%= cat.name %></div>
        <div>Color: <%= cat.color %></div>
        <div>Human: <%= cat.human %></div>
        <img src="<%= cat.image %>" alt="Image of <%= cat.name %>" />
      </article>
      <% } %>
    </div>

    <footer>
